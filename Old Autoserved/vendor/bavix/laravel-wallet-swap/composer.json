{
  "name": "bavix/laravel-wallet-swap",
  "description": "Addition to the package laravel-wallet.",
  "keywords": [
    "swap",
    "laravel",
    "laravel5",
    "laravel6",
    "credits",
    "bavix",
    "currency",
    "laravel5-package",
    "laravel6-package",
    "laravel-wallet",
    "virtual",
    "wallet",
    "payments"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/bavix/laravel-wallet",
  "license": "MIT",
  "authors": [
    {
      "name": "Babichev Maxim",
      "email": "info@babichev.net"
    }
  ],
  "require": {
    "php": "^7.2",
    "bavix/laravel-wallet": "^4.0",
    "florianv/laravel-swap": "^2.0",
    "nyholm/psr7": "^1.1",
    "php-http/curl-client": "^2.0",
    "php-http/message": "^1.7"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "Bavix\\WalletSwap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bavix\\WalletSwap\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bavix\\WalletSwap\\RateServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
